# ViewsBoost Environment Configuration
# Copy this file to .env and fill in your API keys

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# YouTube Data API v3 Keys (Multiple keys for better quota management)
VITE_YOUTUBE_API_KEY_1=your_youtube_api_key_1
VITE_YOUTUBE_API_KEY_2=your_youtube_api_key_2
VITE_YOUTUBE_API_KEY_3=your_youtube_api_key_3
VITE_YOUTUBE_API_KEY_4=your_youtube_api_key_4

# External Stock Photo APIs
VITE_PEXELS_API_KEY=your_pexels_api_key_here
VITE_PIXABAY_API_KEY=your_pixabay_api_key_here
VITE_UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here

# Optional: Additional APIs
VITE_FREEPIK_API_KEY=your_freepik_api_key_here

# Development Settings
VITE_DEV_MODE=true
VITE_LOG_LEVEL=debug
